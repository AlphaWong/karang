{
  "name": "lalamove-ui",
  "version": "0.1.0",
  "description": "React components that implement Lalamove Design.",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "prepare": "npm run build",
    "lint": "npm run lint:css",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint:css": "stylelint \"src/**/*.{css|js}\"",
    "storybook": "start-storybook -p 9002 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "doc": "npx styleguidist server",
    "doc:build": "npx styleguidist build",
    "precommit": "npm run prepare && lint-staged"
  },
  "peerDependencies": {
    "downshift": "^2.0.14",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-moment-proptypes": "^1.6.0",
    "recompose": "^0.27.1",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.5",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/react": "^3.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.4",
    "downshift": "^2.0.14",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest-enzyme": "^6.0.0",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.1.0",
    "npx": "^10.2.0",
    "polished": "^1.9.2",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dates": "^18.0.2",
    "react-dom": "^16.3.2",
    "react-scripts": "^1.1.4",
    "react-styleguidist": "^7.2.3",
    "recompose": "^0.27.1",
    "rollup": "^0.54.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-babel-minify": "^6.0.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "rollup-plugin-url": "^1.3.0",
    "styled-components": "^3.3.3",
    "stylelint": "^9.2.0",
    "stylelint-config-idiomatic-css": "^1.0.5",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/_story.js"
    ]
  },
  "dependencies": {}
}
